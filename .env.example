# Example environment variables for ETAS
# Copy this to `.env` and edit values as needed (this file is safe to commit)

# Initial timer minutes (default: 45)
VITE_INITIAL_MINUTES=45

# Example API URL (client-exposed envs must start with VITE_)
VITE_API_URL=https://api.example.com
 
# Enable dev-only controls in the UI when true (development/testing)
# Set to `true` in your local `.env` to show dev test buttons even outside Vite dev mode
VITE_ENABLE_DEV_CONTROLS=false
